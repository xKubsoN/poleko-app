<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.xkubson.demoapp.controllers.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label styleClass="text" fx:id="welcomeText"/>
    <Label styleClass="text" fx:id="debugTest"/>
    <Label styleClass="text" fx:id="selectedFileLabel"/>
    <Button styleClass="test" text="Hello!" onAction="#onHelloButtonClick"/>
    <Button styleClass="button" text="Select File" onAction="#onFileButtonClick"/>
</VBox>
